<div class="table-responsive table_modify_for_all p_tb_10">
    <table class="table table-bordered m_b_0">
        <thead class="bg_ededed">
        <tr>
            <th><input id="checkAll" type="checkbox"/></th>
            <th>Sr No</th>
            <th>Topic title</th>
            <th>Tag String</th>
            <th>free_text</th>
            <th>Current User</th>
            <th>Stage</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Action</th>


        </tr>
        </thead>
        <tbody>
        {% if disease_all_data %}
            {% for d in disease_all_data %}
                <tr>
                    <td><input type="checkbox" value="{{ d.id }}"></td>
                    <td>{{ forloop.counter }}</td>
                    <td>{{ d.topic_title | capfirst }}</td>
                    <td>{{ d.tag_string | capfirst }}</td>
                    <td>{{ d.free_text | capfirst }}</td>
                    <td>{{ d.current_user.username | capfirst }}</td>
                    <td>{{ d.stage.stage_name | capfirst }}</td>
                    <td>{{ i.createdAt }}</td>
                            <td>{{ i.updatedAt }}</td>
                     <td><a class="btn btn-default btn-sm p_0" href="{% url 'disease-listing-edit' d.id %}"><i
                              class="fa fa-pencil-square-o fa-fw"></i></a> | <a href="{% if d.is_disable %}{% url 'any-master-object-activate' d.id 21 %}{% else %}{% url 'any-master-object-deactivate' d.id 21 %}{% endif %}"
                                                                                      class="btn btn-default btn-sm">
                                {% if d.is_disable %}Enable {% else %}Disable {% endif %}</a>
<span class="p_lr_10"></span>
                             <a class="btn btn-danger btn-sm p_0"
                               href="{% url 'delete_disease' i.id %}"><i
                                    class="fa fa-trash-o text-danger fa-fw text_white"></i></a>
                     </td>

                </tr>
            {% endfor %}
        {% else %}
            <tr>
                <td colspan="7"><b>Disease's Name could not be found in system...<span class='fa fa-frown-o fa-lg'></span></b></td>
            </tr>
        {% endif %}
        </tbody>
    </table>
    {% if disease_all_data %}
                <div class="text-right">
                    <ul class="pagination">
                        {% if disease_all_data.has_previous %}
                            <li class="prev"><a href="?page={{ disease_all_data.previous_page_number }}{% if search_data %}&search_data={{search_data}}{% endif %}">Prev</a></li>
                        {% endif %}
                        <li><a>Page {{ disease_all_data.number }} of {{ disease_all_data.paginator.num_pages }}</a></li>
                        {% if disease_all_data.has_next %}
                            <li class="next"><a href="?page={{disease_all_data.next_page_number }}{% if search_data %}&search_data={{search_data}}{% endif %}">Next</a></li>
                        {% endif %}
                    </ul>
                </div>
            {% endif %}
<script>
        $("#checkAll").change(function () {
            $("input:checkbox").prop('checked', $(this).prop("checked"));
        });
        var $rows = $('#table tr');
        $('#search').keyup(function () {
            var val = $.trim($(this).val()).replace(/ +/g, ' ').toLowerCase();

            $rows.show().filter(function () {
                var text = $(this).text().replace(/\s+/g, ' ').toLowerCase();
                return !~text.indexOf(val);
            }).hide();
        });


    </script>
</div>