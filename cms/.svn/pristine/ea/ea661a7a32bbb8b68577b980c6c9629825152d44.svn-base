                                            <option value="{{ b.id }}">{{ b.username }}</option>

                                        {% endfor %}
                                    </select>
                                </div>
                              <div class="col-md-4">
                                        <select class="form-control" name="validator_name" id="validator_name">
                                        <option value="" disabled>Select Validation Choice</option>
                                            {% for e in valid_choice %}

                                                <option value="{{ e.id }}">{{ e.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>

                                <div class="col-md-4">
                                    <button class="btn btn-info" onclick="doctor_move_to_next_stage()">Move to Next
                                        Stage
                                    </button>
                                </div>

                            </div>
                        {% endif %}
                    </div>
                </div>


