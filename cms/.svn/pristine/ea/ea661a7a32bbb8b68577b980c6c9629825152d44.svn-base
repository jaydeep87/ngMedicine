                                                {% if b.is_doctor_reviewer and b.user.is_active %}

                                                    <option value="{{ b.user_id }}">{{ b.user.username }}</option>
                                            {% endif %}
                                            {% endfor %}
                                        </select>
                                    </div>
                                    <div class="col-md-4">
                                        <button class="btn btn-info" type="submit">Reverse to Reviewer</button>
                                    </div>



                                     </form>
                                </div>
                                </div>
                  </div>

                  <div class="thumbnail p_10">
                <div class="row">
                        <div class="col-md-12 m_b_15">
                            <form role="form" action="  " method="post">
                                <input type="hidden" value=" {#  #}" name="doctor_id">
                                <input type="hidden" value="TrUe" name="live_doc_true" id="live_doc_true">

                                <button type="submit" value="submit" class="btn btn-info">Publish To Live
                                    </button>




                            </form>
                        </div>
                </div>
                </div>
                  {% endif %}



