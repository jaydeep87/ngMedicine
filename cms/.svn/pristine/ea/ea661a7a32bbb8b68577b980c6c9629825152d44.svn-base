<form action="{% url 'live_doctor_listing_edit' doctor_id %}?tab=1" method="post">
<div class="row">

<div class="col-md-5">
<label>First Name<span style="color: red;size: 25px;">*</span> </label>
<div class="form-group form-group-lg"><input type="text" class="form-control" value= "{{ live_doctor.firstName }}" name="firstName" maxlength="30" minlength="1" pattern="[a-zA-Z ]+" title="Only charecter can be entered " readonly></div>
<label>Last Name<span style="color: red;size: 25px;">*</span> </label>
<div class="form-group form-group-lg"><input type="text" class="form-control" value= "{{ live_doctor.lastName }}" name="lastName" maxlength="30" minlength="1" pattern="[a-zA-Z ]+" title="Only charecter can be entered " readonly></div>

<div class="form-group form-group-lg">
<label>Category:<span style="color: red;size: 25px;">*</span> </label>
<select id="category" class="chosen-select" name="category">
{#    {% if request.user.is_staff %} disabled{% endif %}>#}
    <option value="">--Select Category--</option>
    {% if category_obj %}
        {% for u in category_obj %}
            {% if u.id|slugify == curent_cat|slugify %}
            <option value="{{ u.id }}" selected>{{ u.name }}</option>
            {% else %}
            <option value="{{ u.id }}" >{{ u.name }}</option>
            {% endif %}
        {% endfor %}
    {% endif %}
</select>
</div>


<div class="form-group multi_selections">
    <label>Service Offered</label>
    <select id="serviceoffer" name="serviceoffer" class="form-control chosen-select"
            multiple disabled="disabled">
        {% if serviceOffered %}
            {% for so in serviceOffered %}
                {% if so in current_seroff %}
                    <option value="{{ so.id }}"selected>{{ so.name }}</option>
{#                {% else %}#}
{#                    <option value="{{ so.id }}">{{ so.name }}</option>#}
                {% endif %}
            {% endfor %}
        {% endif %}
    </select>
</div>

,

<div class="form-group multi_selections">
    <label>New Service Offered Final</label>
    <select id="serviceoffer3" name="serviceoffer3" class="form-control chosen-select"
            multiple >
        {% if New_SO_Final_in_association %}
            {% for so in New_SO_Final_in_association %}
                {% if so.id in Current_New_SO_Final_in_live_doctor %}
                    <option value="{{ so.id }}"selected>{{ so.name }}</option>
                {% else %}
                    <option value="{{ so.id }}">{{ so.name }}</option>
                {% endif %}
            {% endfor %}
        {% endif %}
    </select>
</div>



</div>

<div class="col-md-7">
<div class="row">
<div class="col-md-8">


<div class="form-group">
    <label>DOB<span style="color: red;size: 25px;">*</span></label>
    <input type="text"  value = "{{ live_doctor.dob }}" class="form-control" name="DOB" required id="datePicker">
</div>


<div class="row form-group" style="height:36px;">

<div class="form-group">
    <label>Gender:<span style="color: red;size: 25px;">*</span> </label>
    <label class="radio-inline" style="padding-top: 0;">
        <input type="radio" id="male" value="male" name="gender" {% if live_doctor.gender == 'male' %}checked="checked"{% endif %}> Male
    </label>
    <label class="radio-inline" style="padding-top: 0;">
        <input type="radio" id="female" value="female" name="gender" {% if live_doctor.gender == 'female' %}checked="checked"{% endif %}>
        Female
    </label>
</div>


</div>
</div>

</div>

<div class="form-group">
    <label>Speciality</label>
    <!-- <select id="specialty" name="specialty" class="form-control chosen-select" multiple> -->
    <select id="specialty" name="specialty" class="form-control chosen-select" multiple disabled="disabled" >
        {% if speciality %}
            {% for spe in speciality %}
                 {% if spe in current_specialities %}
                    <option value="{{ spe.id }}" selected>{{ spe.name }}</option>
{#                 {% else %}#}
{#                    <option value="{{ spe.id }}">{{ spe.name }}</option>#}
                 {% endif %}
            {% endfor %}
        {% endif %}
    </select>
 </div>



<div class="form-group">
    <label>New Speciality Final</label>
    <!-- <select id="specialty" name="specialty" class="form-control chosen-select" multiple> -->
    <select id="specialty3" name="specialty3" class="form-control chosen-select" multiple  >
        {% if New_SPE_Final_in_association %}
            {% for asso_spe in New_SPE_Final_in_association %}
                {% if asso_spe.id in Current_New_SPE_Final_in_live_doctor %}
                        <option value="{{ asso_spe.id }}" selected>{{ asso_spe.name }}</option>
                {% else %}
                        <option value="{{ asso_spe.id }}">{{ asso_spe.name }}</option>
                {% endif %}
            {% endfor %}
        {% endif %}
    </select>
 </div>

<div class="form-group">
<label>Zone</label>
   <select id="zone" class="chosen-select" name="zone">
                    <!--{% if request.user.is_staff %} disabled{% endif %}>-->
                <option value="">--Select Zone--</option>
                {% if zone_obj %}
                    {% for r in zone_obj %}
                        {% if current_zone %}
                            {% if r.id == current_zone.id %}
                            <option value="{{ r.id }}" selected>{{ r.name }}</option>
                            {% else %}
                            <option value="{{ r.id }}">{{ r.name }}</option>
                            {% endif %}
                        {% else %}
                         <option value="{{ r.id }}">{{ r.name }}</option>
                        {% endif %}
                    {% endfor %}
                {% endif %}
   </select>
</div>


<div class="form-group">
<label>Zone Location</label>
    <select id="zone_location" class="chosen-select" name="zone_location">
                <!--{% if request.user.is_staff %} disabled{% endif %}>-->
            <option value="">--Select Zone Location--</option>
            {% if current_zone and zone_based_locations %}
                {% for i in zone_based_locations %}
                   {% if live_doctor.zone_location.id == i.id %}
                       <option value="{{ i.id }}" selected>{{ i.name }}</option>
                   {% else %}
                       <option value="{{ i.id }}">{{ i.name }}</option>
                   {% endif %}
                {% endfor %}

            {% elif current_zone %}
               <option value="{{ current_zone.id }}" selected>{{ current_zone.name }}</option>
            {% else %}
               <option value=""></option>
            {% endif %}




    </select>
</div>

<div class="form-group">
    <label>DID</label>
    <input type="text" maxlength="11" class="form-control" name="live_did" placeholder="DID" value="{{ live_doctor.did }}">
</div>

<div class="form-group">
    <label>Extension</label>
    <input type="text" maxlength="4" class="form-control" name="live_extension" placeholder="Extension" value="{{ live_doctor.extension }}">
</div>


</div>

</div>
<!--ONE END-->

<p>&nbsp;</p>
