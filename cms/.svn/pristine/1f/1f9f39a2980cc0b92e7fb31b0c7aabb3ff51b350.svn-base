 <div class="table-responsive table_modify_for_all">
            <table class="table table-bordered">
                <thead>
                <tr>
                    <th>Sr No</th>
                    <th>Blood Bank Name</th>
                    <th>City</th>
                    <th>Location</th>
                    <th>Stage</th>
                    <th>Current User</th>
                    <th>Created At</th>
                    <th>Updated At</th>
                    <th>Action</th>

                </tr>
                </thead>
                <tbody>
                {% if bloodbank_all_data %}
                {% for i in bloodbank_all_data %}
                    <tr>
                    <td>{{ forloop.counter }}</td>
                    <td>{{ i.name | capfirst }}</td>
                    <td>{{ i.city.name | capfirst }}</td>
                    <td>{{ i.locality.name | capfirst }}</td>
                    <td>{{ i.stage.stage_name | capfirst}}</td>
                    <td>{{ i.current_user.username |capfirst }}</td>
                    <td>{{ i.createdAt }}</td>
                    <td>{{ i.updatedAt }}</td>
                    <td>
                         <a class="btn btn-default btn-sm p_0" href="{% url 'time-schedule-bloodbank' i.id %}"><i class="fa fa-clock-o fa-fw text_333"></i></a>
                        <button type="button" class="btn text_999 btn-sm" data-toggle="modal" data-target="#editMode"><i
                                class="fa fa-edit"></i></button>
                    </td>
                {% endfor %}
                {% else %}
                    <tr>
                    <td colspan="5"><b>Blood Bank's Name could not be found in system...<span class='fa fa-frown-o fa-lg'></span></b></td>
                    </tr>
                {% endif %}
                </tbody>

            </table>
 {% if bloodbank_all_data %}
                <div class="text-right">
                    <ul class="pagination">
                        {% if bloodbank_all_data.has_previous %}
                            <li class="prev"><a href="?page={{ bloodbank_all_data.previous_page_number }}{% if search_data_two %}&search_data_two={{search_data_two}}{% endif %}">Prev</a></li>
                        {% endif %}
                        <li><a>Page {{ bloodbank_all_data.number }} of {{ bloodbank_all_data.paginator.num_pages }}</a></li>
                        {% if bloodbank_all_data.has_next %}
                            <li class="next"><a href="?page={{ bloodbank_all_data.next_page_number }}{% if search_data_two %}&search_data_two={{search_data_two}}{% endif %}">Next</a></li>
                        {% endif %}
                    </ul>
                </div>
            {% endif %}
        </div>