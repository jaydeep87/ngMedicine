<div class="table-responsive table_modify_for_all">
    <table class="table table-bordered">
        <thead>
        <tr>
            <th><input id="checkAll" type="checkbox"/></th>
            <th>Sr No</th>
            <th>Rehab Name</th>
            <th>City</th>
            <th>Location</th>
            <th>Stage</th>
            <th>Current User</th>
            <th>Created At</th>
                    <th>Updated At</th>
            <th>Action</th>
        </tr>
        </thead>
        <tbody>
        {% if rehab %}
            {% for i in rehab %}
                <tr>
                    <td><input type="checkbox" value="{{ i.id }}"></td>
                    <td>{{ forloop.counter }}</td>
                    <td>{{ i.clinic_name | capfirst }}</td>
                    <td>{{ i.city.name | capfirst }}</td>
                    <td>{{ i.locality.name | capfirst }}</td>

                    <td>{{ i.stage.stage_name | capfirst }}</td>

                    <td>{{ i.current_user.username | capfirst }}</td>
                    <td>{{ i.created_at }}</td>
                    <td>{{ i.updatedAt }}</td>

                    <td>
                        <a class="btn btn-default btn-sm p_0" href="{% url 'time-schedule-rehab' i.id %}"><i class="fa fa-clock-o fa-fw text_333"></i></a>
                        <a class="text_white" href="{% url 'rehab-listing-edit' i.id %}?tab=1"><i class="fa fa-edit"></i></a>
                    </td>
                </tr>
            {% endfor %}
        {% else %}
            <tr>
                <td colspan="5"><b>Rehab's Name could not be found in system...<span
                        class='fa fa-frown-o fa-lg'></span></b>
                </td>
            </tr>
        {% endif %}
        </tbody>

    </table>
    {% if rehab %}
        <div class="text-right">
            <ul class="pagination">
                {% if rehab.has_previous %}
                    <li class="prev"><a href="?page={{ rehab.previous_page_number }}">Prev</a></li>
                {% endif %}
                <li><a>Page {{ rehab.number }} of {{ rehab.paginator.num_pages }}</a></li>
                {% if rehab.has_next %}
                    <li class="next"><a href="?page={{ rehab.next_page_number }}">Next</a></li>
                {% endif %}
            </ul>
        </div>
    {% endif %}
</div>