<div class="table-responsive table_modify_for_all">
    <table class="table table-bordered">
        <thead>
        <tr>
            <th>Sr No</th>
            <th>Organisation Name</th>
            <th>City</th>
            <th>Locality</th>
            <th>Stage</th>
            <th>Current User</th>
             <th>Created At</th>
            <th>Updated At</th>
            <th>Action</th>

        </tr>
        </thead>
        <tbody>
        {% for i in organisation_all_data %}
            <tr>
            <td>{{ forloop.counter }}</td>
            <td>{{ i.name | capfirst }}</td>
            <td>{{ i.city.name }}</td>
            <td>{{ i.locality.name }}</td>
            <td>{{ i.stage.stage_name | capfirst }}</td>
            <td>{{ i.current_user.username | capfirst }}</td>
                <td>{{ i.createdAt }}</td>
                    <td>{{ i.updatedAt }}</td>
            <td><a class="btn btn-default btn-sm p_0"
                                       href="{% url 'time-schedule-hospital' i.id %}"><i
                                            class="fa fa-clock-o fa-fw text_333"></i></a>
                <a class="btn btn-default btn-sm p_0" href="{% url 'organisation-listing-edit' i.id %}"><i
                    class="fa fa-pencil-square-o fa-fw"></i></a></td>
        {% endfor %}
        </tbody>

    </table>
    {% if organisation_all_data %}
        <div class="text-right">
            <ul class="pagination">
                {% if organisation_all_data.has_previous %}
                    <li class="prev"><a href="?page={{ organisation_all_data.previous_page_number }}{% if stage_no %}&stage_id={{stage_no}}{% endif %}">Prev</a></li>
                {% endif %}
                <li><a>Page {{ organisation_all_data.number }} of {{ organisation_all_data.paginator.num_pages }}</a>
                </li>
                {% if organisation_all_data.has_next %}
                    <li class="next"><a href="?page={{ organisation_all_data.next_page_number }}{% if stage_no %}&stage_id={{stage_no}}{% endif %}">Next</a></li>
                {% endif %}
            </ul>
        </div>
    {% endif %}
</div>