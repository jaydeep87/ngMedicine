{% extends 'global_search/doctor/doctor_global_search.html' %}
{% load staticfiles %}
{% load template_tag %}

{% block doctor_data %}
    {% if messages %}
        {% for message in messages %}
            <div class="alert {% if message.tags == 'error' %}alert-danger{% elif message.tags == 'success' %}alert-success{% endif %} m_tb_10"
                 id="alert">{{ message }}</div>
        {% endfor %}
    {% endif %}
    <fieldset {% if request.user|can_view_review %}{% endif %} disabled>
        <div class="border_5_light_white p_10">
            <h4 class="font_weight_600 text_white m_t_0"><i class="fa fa-hand-o-right fa-fw"></i> Reward &amp;
                Recognization</h4>
            <!--COLLAPSE START-->
            <!--COLLAPSE END-->
            <div class="table-responsive table_modify_for_all p_tb_10">
                <table class="table table-bordered m_b_0">
                    <thead class="bg_ededed">
                    <tr>
                        <th>Sr. No.</th>
                        <th>Reward &amp; Recognization</th>
                        <th>Created At</th>
                        <th>Updated At</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for i in reward_data %}
                        <tr>

                            <td>{{ forloop.counter }}</td>
                            <td>{{ i.reward_data }}</td>
                            <td>{{ i.createdAt }}</td>
                            <td>{{ i.updatedAt }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>

            </div>
        </div>
        <div class="border_5_light_white p_10">
            <h4 class="font_weight_600 text_white m_t_0"><i class="fa fa-hand-o-right fa-fw"></i> Membership</h4>


            <!--COLLAPSE START-->
            <!--COLLAPSE END-->
            <div class="table-responsive table_modify_for_all p_tb_10">
                <table class="table table-bordered m_b_0">
                    <thead class="bg_ededed">
                    <tr>
                        <th>Sr. No.</th>
                        <th>Membership</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for k in membership_data %}
                        <tr>
                            <td>{{ forloop.counter }}</td>
                            <td>{{ k.name }}</td>
                        </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </fieldset>


    <script>
        $('#alert').fadeIn().delay(2000).fadeOut('slow');
    </script>
{% endblock %}