<div class="table-responsive table_modify_for_all">
    <table class="table table-bordered">
        <thead>
        <tr>
            <th>Sr No</th>
            <th>Rehab Name</th>
            <th>City</th>
            <th>Location</th>
            <th>Stage</th>
            <th>Current User</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Action</th>
        </tr>
        </thead>
        <tbody>
        {% if rehab_all_data %}
            {% for i in rehab_all_data %}
                <tr>
                    <td>{{ forloop.counter }}</td>
                    <td>{{ i.clinic_name | capfirst }}</td>
                    <td>{{ i.city.name | capfirst }}</td>
                    <td>{{ i.locality.name | capfirst }}</td>
                    <td>{{ i.stage.stage_name | capfirst }}</td>
                    <td>{{ i.current_user.username | capfirst }}</td>
                    <td>{{ i.createdAt }}</td>
                    <td>{{ i.updatedAt }}</td>
                    <td><a class="btn btn-default btn-sm p_0" href="{% url 'time-schedule-rehab' i.id %}"><i class="fa fa-clock-o fa-fw text_333"></i></a>
                        <a class="btn btn-default btn-sm p_0" href="{% url 'rehab-listing-edit' i.id %}?tab=1"><i
                            class="fa fa-pencil-square-o fa-fw"></i></a></td>
                </tr>
            {% endfor %}
        {% else %}
            <tr>
                <td colspan="5"><b>Rehabs could not be found for selected user...<span
                        class='fa fa-frown-o fa-lg'></span></b>
                </td>
            </tr>

        {% endif %}

        </tbody>

    </table>
    {% if rehab_all_data %}
        <div class="text-right">
            <ul class="pagination">
                {% if rehab_all_data.has_previous %}
                    <li class="prev"><a href="?page={{ rehab_all_data.previous_page_number }}{% if search_data %}&search_data={{search_data}}{% endif %}">Prev</a></li>
                {% endif %}
                <li><a>Page {{ rehab_all_data.number }} of {{ rehab_all_data.paginator.num_pages }}</a></li>
                {% if rehab_all_data.has_next %}
                    <li class="next"><a href="?page={{ rehab_all_data.next_page_number }}{% if search_data %}&search_data={{search_data}}{% endif %}">Next</a></li>
                {% endif %}
            </ul>
        </div>
    {% endif %}
</div>