<div class="table-responsive table_modify_for_all">
    <table class="table table-bordered">
        <thead>
        <tr>
            <th><input id="checkAll" type="checkbox"/></th>
            <th>Sr No</th>
            <th>Category Name</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Action</th>
        </tr>
        </thead>
        <tbody>
        {% if category_obj%}
            {% for i in category_obj %}

                <tr>
                    <td><input type="checkbox" value="{{ i.id }}"></td>
                    <td>{{ forloop.counter }}</td>
                    <td>{{ i.name | capfirst }}</td>
                    <td>{{ i.createdAt }}</td>
                    <td>{{ i.updatedAt }}</td>
                    <td><a class="text_white" href="{% url 'category_edit' i.id %}"><i
                            class="fa fa-edit"></i></a><span class="p_lr_10">|</span>{% if i.delete == False %}<a href="{% url 'any-master-object-deactivate' object_id=i.id object_type=5 %}" class="text_white"><i class="fa fa-close"></i></a>
                    {% else %}<a href="{% url 'any-master-object-activate' object_id=i.id object_type=5 %}" class="text_white"><i class="fa fa-check-square-o"></i></a>{% endif %}</td>
                </tr>

            {% endfor %}
        {% else %}
            <tr><td> No Category Found </td></tr>
        {% endif %}
        </tbody>

    </table>
