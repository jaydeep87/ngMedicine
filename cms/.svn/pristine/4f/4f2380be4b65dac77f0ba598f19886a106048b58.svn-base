	}

.img-wrap {
    position: relative;
    display: inline-block;
    border: 2px white solid;
    font-size: 0;
}
.img-wrap .close {
    position: absolute;
    top: 2px;
    right: 2px;
    z-index: 100;
    background-color: #FFF;
    padding: 5px 2px 2px;
    color: #000;
    font-weight: bold;
    cursor: pointer;
    opacity: .2;
    text-align: center;
    font-size: 22px;
    line-height: 10px;
    border-radius: 50%;
}
.img-wrap:hover .close {
    opacity: 1;
}

.jd > form{width:175px; height: 150px; display: inline-block; margin: 10px;}
.jd > form img{width: 100%; height: 150px;}

.media_circle {
    background: #19a3e7 none repeat scroll 0 0;
    border-radius: 100%;
    color: #fff;
    font-size: 18px;
    font-weight: 700;
    height: 40px;
    margin-right: 20px;
    padding-top: 5px;
    text-align: center;
    text-transform: uppercase;
    width: 40px;
}

.label_black label{color:#666;}
