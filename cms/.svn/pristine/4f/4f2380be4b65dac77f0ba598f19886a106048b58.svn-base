/*ALL CSS START*/
.login_body{background:url(../images/hfu_bg.jpg) no-repeat center top/cover; background-attachment:fixed;)}
.login_slowgan h1{font-size:70px; color:#000; text-transform:uppercase; font-weight:300; text-shadow:0 1px 3px #fff; }
.login_panel{background:rgba(0,0,0,0.3); min-height:400px; border:5px solid rgba(0,0,0,0.1);}
.login_panel .nav-tabs{border:0;}

.login_panel .nav-tabs > li > a{background:rgba(1,155,184,0.5); color:#fff; font-size:18px; outline:0; border:0;}
.login_panel .nav-tabs > li.active > a, .login_panel .nav-tabs > li.active > a:focus, .login_panel .nav-tabs > li.active > a:hover{background:#F26932; color:#fff; outline:0; border:0;}

.social_media{display:inline-block; width:40%; padding-left:12%;}

header .lr .dropdown-menu{padding:0;}
header .lr .dropdown-menu > li > a{padding:10px; font-size:16px; border-bottom:1px dotted #e5e5e5;}
header .lr .dropdown-menu > li > a:hover{padding:10px; font-size:16px; background:rgba(236, 151, 31, 0.7); -moz-transition:all 0.5s ease; -ms-transition:all 0.5s ease; -o-transition:all 0.5s ease; -webkit-transition:all 0.5s ease; transition:all 0.5s ease; color:#fff;}

.breadcrumb_for_all .breadcrumb{background:#f5f5f5; border:2px solid rgba(256, 256, 256, 0.5);}

.all_container{background:rgba(0, 0, 0, 0.1); border:3px solid rgba(0, 0, 0, 0.1); min-height:400px; padding:20px;}
.all_container .nav-pill{border:1px dashed #666; border-radius:0 10px 0 10px;}
.all_container .nav > li > a{font-size:15px; color:#000; border-bottom:1px dashed #999; background:rgba(1, 155, 184, 0.5); -moz-transition:all 0.7s ease; -ms-transition:all 0.7s ease; -o-transition:all 0.7s ease; -webkit-transition:all 0.7s ease; transition:all 0.7s ease; margin:3px 2px;}
.tb_five .nav{border:0;}
.tb_five .form-group label{font-weight:400; color:#fff; font-size:15px;}
.tb_five .nav > li > a{background:rgba(1, 155, 184, 0.8); font-size:14px; font-weight:600;}
.all_container .nav > li > a:hover,.all_container .nav > li.active a,.all_container .nav > li > a:focus{background:rgba(236, 151, 31, 0.8); color:#fff; outline:0;}
.all_container .nav > li.active a.first_child{border-radius:0 10px 0 0;}
.all_container .nav > li.active a.last_child{border-radius:0 0 0 10px;}

.four_member .thumbnail{border-radius:5px; padding:20px 0; background:rgba(1,155,184,0.5); transition:all 0.5s ease; box-shadow:0 2px 3px #666;}
.four_member .label{position:absolute; top:-8px; right:5px;}
.four_member a.thumbnail{text-decoration:none; cursor:pointer; color:#fff; border:0;}
.four_member a.thumbnail:hover{border:0; background:rgba(236,151,31,0.9);}
.four_member a.thumbnail figure{text-align:center;}
.four_member h3{text-align:center;}

.table_modify_for_all .table > thead > tr > th{text-align:center; background:rgba(0,0,0,0.5); color:#fff;} 
.table_modify_for_all .table > tbody > tr > td{text-align:center; background:rgba(0,0,0,0.2); color:#fff; word-break: break-all;}

.tableschedule{border:1px solid #e5e5e5; padding:10px; min-height:60px; overflow:hidden;}
.tableschedule .left_schedule{width:300px; display:inline-block; float:left; padding-top:3px; }
.tableschedule .right_schedule{width:450px; display:inline-block; text-align:right; float:right; margin:3px 0;}

.tableschedule .value_left{width:100px; display:inline-block; position:relative; z-index:2;}
.tableschedule .value_right{width:80px; display:inline-block; margin-left:-10px; position:relative; z-index:1;}
.tableschedule .seprator{width:30px; display:inline-block; text-align:center; color:#666; font-weight:bold;}

.plus_icon{display:inline-block; font-size:20px; color:#fff; width:60px; text-align:center; float:right; padding-top:29px;}
.plus_icon button{background:inherit; border:0; padding:0; margin:3px;}

/*ALL CSS END*/








