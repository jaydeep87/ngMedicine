 <div class="table-responsive table_modify_for_all">
            <table class="table table-bordered">
                <thead>
                <tr>
                    <th>Sr No</th>
                    <th>Topic title</th>
                    <th>Tag String</th>
                    <th>free_text</th>
                    <th>Current User</th>
                    <th>Stage</th>
                    <th>Created At</th>
                    <th>Updated At</th>
                    <th class="col-md-1">Action</th>
                </tr>
                </thead>
                <tbody>
                {% if symptoms_all_data %}
                    {% for i in symptoms_all_data %}
                        <tr>
                        <td>{{ forloop.counter }}</td>
                        <td>{{ i.topic_title | capfirst }}</td>
                        <td>{{ i.tag_string | capfirst }}</td>
                        <td>{{ i.free_text | capfirst }}</td>
                        <td>{{ i.current_user.username | capfirst }}</td>
                        <td>{{ i.stage.stage_name | capfirst }}</td>
                        <td>{{ i.createdAt }}</td>
                        <td>{{ i.updatedAt }}</td>
                        <td>
                            <a class="text_white" href=" {% url 'symptoms-listing-edit' i.id %}"><button type="button" class="btn text_999 btn-sm" data-toggle="modal"
                                    data-target="#editMode"><i
                                    class="fa fa-edit"></i></button></a>

                        <span class="p_lr_10"></span>
                             <a class="btn btn-danger btn-sm p_0"
                               href="{% url 'delete_symptoms' i.id %}"><i
                                    class="fa fa-trash-o text-danger fa-fw text_white"></i></a>

                        </td>
                {% endfor %}
                {% else %}
                    <tr>
                    <td colspan="5"><b>Symptoms's Name could not be found in system...<span class='fa fa-frown-o fa-lg'></span></b></td>
                    </tr>
                {% endif %}
                </tbody>

            </table>
 {% if symptoms_all_data %}
                <div class="text-right">
                    <ul class="pagination">
                        {% if symptoms_all_data.has_previous %}
                            <li class="prev"><a href="?page={{ symptoms_all_data.previous_page_number }}{% if search_data_two %}&search_data_two={{search_data_two}}{% endif %}">Prev</a></li>
                        {% endif %}
                        <li><a>Page {{ symptoms_all_data.number }} of {{ symptoms_all_data.paginator.num_pages }}</a></li>
                        {% if symptoms_all_data.has_next %}
                            <li class="next"><a href="?page={{ symptoms_all_data.next_page_number }}{% if search_data_two %}&search_data_two={{search_data_two}}{% endif %}">Next</a></li>
                        {% endif %}
                    </ul>
                </div>
            {% endif %}
        </div>