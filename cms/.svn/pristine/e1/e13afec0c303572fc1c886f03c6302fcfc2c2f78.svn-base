<div class="table-responsive table_modify_for_all p_tb_10">
    <table class="table table-bordered m_b_0">
        <thead class="bg_ededed">
        <tr>

            <tr>
                    <th><input id="checkAll" type="checkbox"/></th>
                    <th>Sr No</th>
                    <th>Plan Name</th>
                    <th>Current User</th>
                    <th>Stage</th>
                    <th>Provider Name</th>
                    <th>Free Text</th>
                    <th>Created At</th>
                    <th>Updated At</th>
                    <th>Action</th>
                </tr>
                </thead>
                <tbody>
                {% if life_plan_all_data %}
                    {% for i in life_plan_all_data%}
                        <tr>
                        <td><input type="checkbox" value="{{ i.id }}"></td>
                        <td>{{ forloop.counter }}</td>
                        <td>{{ i.plan_name | capfirst }}</td>
                        <td>{{ i.current_user.username | capfirst }}</td>
                        <td>{{ i.stage.stage_name| capfirst }}</td>
                        <td>{{ i.provider.owner_name| capfirst }}</td>
                        <td>{{ i.free_text | capfirst }}</td>
                        <td>{{ i.createdAt }}</td>
                        <td>{{ i.updatedAt }}</td>
                        <td>
                            <a class="text_white" href="  {% url 'life-service-plan-edit' i.id %} "><i
                                    class="fa fa-edit"></i></a><span class="p_lr_10">|</span>{% if i.activation_status == True %}<a href="{% url 'any-master-object-deactivate' object_id=i.id object_type=15 %}" class="text_white"><i class="fa fa-close"></i></a>
                            {% else %}<a href="{% url 'any-master-object-activate' object_id=i.id object_type=15 %}  " class="text_white"><i class="fa fa-check-square-o"></i></a>{% endif %}
                        </td>

                </tr>
            {% endfor %}
        {% else %}
            <tr>
                <td colspan="7"><b>Life plan's Name could not be found in system...<span class='fa fa-frown-o fa-lg'></span></b></td>
            </tr>
        {% endif %}
        </tbody>
    </table>
    {% if life_plan_all_data %}
                <div class="text-right">
                    <ul class="pagination">
                        {% if life_plan_all_data.has_previous %}
                            <li class="prev"><a href="?page={{ life_plan_all_data.previous_page_number }}{% if search_data %}&search_data={{search_data}}{% endif %}">Prev</a></li>
                        {% endif %}
                        <li><a>Page {{ life_plan_all_data.number }} of {{ life_plan_all_data.paginator.num_pages }}</a></li>
                        {% if life_plan_all_data.has_next %}
                            <li class="next"><a href="?page={{life_plan_all_data.next_page_number }}{% if search_data %}&search_data={{search_data}}{% endif %}">Next</a></li>
                        {% endif %}
                    </ul>
                </div>
   {% endif %}

<script>
        $("#checkAll").change(function () {
            $("input:checkbox").prop('checked', $(this).prop("checked"));
        });
        var $rows = $('#table tr');
        $('#search').keyup(function () {
            var val = $.trim($(this).val()).replace(/ +/g, ' ').toLowerCase();

            $rows.show().filter(function () {
                var text = $(this).text().replace(/\s+/g, ' ').toLowerCase();
                return !~text.indexOf(val);
            }).hide();
        });


    </script>

</div>