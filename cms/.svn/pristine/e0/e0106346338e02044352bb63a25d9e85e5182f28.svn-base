                $("#in2").remove();
                clone_indexb--;
            }
        });

        $("#btn5").click(function () {
            $("#wrap3").append('<div id = "in3"  class="form-group col-md-12"><input type="text" name="diabetes_evaluation" class="form-control" placeholder="Diabetes Evaluation" ></div>');
            clone_indexc++;
