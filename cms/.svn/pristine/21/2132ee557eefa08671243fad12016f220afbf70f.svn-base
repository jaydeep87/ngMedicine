<div class="tab-pane photo {% if tab  == '1' %} in active {% else %} fade {% endif %}" id="basic_profiless">



<h1 class="login_detils position_relative"><span class="font_weight_700 text_666">Basic Details</span></h1>
<p>&nbsp;</p>

<!--ONE START-->
<div class="row">
<form action="{% url 'upload_profile_pic' doctor_id %}" method="post" enctype="multipart/form-data">{% csrf_token %}
<div class="col-md-6 col-sm-6">
<h5 class="font_weight_600">Upload Profile image</h5>
<div class="m_tb_10"><img class="img-thumbnail" style="width: 200px; height: 180px;" src="{{ hostport }}{{ live_doctor.profileImage }}"></img>   </div>
<!--<input id="ProfileImage" type="file" class="" name="ProfileImage"-->
<!--<button  style="margin-top: 15px;!important;" type="submit" class="btn btn-success ">Upload</button>-->
<p>&nbsp;</p>
</div>
</form>
</div>



<form action="{% url 'live_doctor_listing_edit' doctor_id %}?tab=1" method="post">
<div class="row">

<div class="col-md-5">
<label>First Name<span style="color: red;size: 25px;">*</span> </label>
<div class="form-group form-group-lg"><input type="text" class="form-control" value= "{{ live_doctor.firstName }}" name="firstName" maxlength="30" minlength="1" pattern="[a-zA-Z ]+" title="Only charecter can be entered " readonly></div>
<label>Last Name<span style="color: red;size: 25px;">*</span> </label>
<div class="form-group form-group-lg"><input type="text" class="form-control" value= "{{ live_doctor.lastName }}" name="lastName" maxlength="30" minlength="1" pattern="[a-zA-Z ]+" title="Only charecter can be entered " readonly></div>

<div class="form-group form-group-lg">
<label>Category:<span style="color: red;size: 25px;">*</span> </label>
<select id="category" class="chosen-select" name="category" disabled>
{#    {% if request.user.is_staff %} disabled{% endif %}>#}
    <option value="">--Select Category--</option>
    {% if category_obj %}
        {% for u in category_obj %}
            {% if u.id|slugify == curent_cat|slugify %}
            <option value="{{ u.id }}" selected>{{ u.name }}</option>
            {% else %}
            <option value="{{ u.id }}" >{{ u.name }}</option>
            {% endif %}
        {% endfor %}
    {% endif %}
</select>
</div>


<div class="form-group multi_selections">
    <label>Service Offered</label>
    <select id="serviceoffer" name="serviceoffer" class="form-control chosen-select"
            multiple disabled>
        {% if serviceOffered %}
            {% for so in serviceOffered %}
                {% if so in current_seroff %}
                    <option value="{{ so.id }}"selected>{{ so.name }}</option>
                {% else %}
                    <option value="{{ so.id }}">{{ so.name }}</option>
                {% endif %}
            {% endfor %}
        {% endif %}
    </select>
</div>

</div>

<div class="col-md-7">
<div class="row">
<div class="col-md-8">


<div class="form-group">
    <label>DOB</label>
    <input type="text"  value = "{{ live_doctor.dob }}" class="form-control" name="DOB" required id="datePicker" readonly>
</div>


<div class="row form-group" style="height:36px;">

<div class="form-group">
    <label>Gender:<span style="color: red;size: 25px;">*</span> </label>
    <label class="radio-inline" style="padding-top: 0;">
        <input type="radio" id="male" value="male" name="gender" {% if live_doctor.gender == 'male' %}checked="checked"{% endif %} readonly> Male
    </label>
    <label class="radio-inline" style="padding-top: 0;">
        <input type="radio" id="female" value="female" name="gender" {% if live_doctor.gender == 'female' %}checked="checked"{% endif %} readonly>
        Female
    </label>
</div>


</div>
</div>

</div>

<div class="form-group">
    <label>Speciality</label>
    <select id="specialty" name="specialty" class="form-control chosen-select" multiple disabled>
        {% if speciality %}
            {% for spe in speciality %}
                 {% if spe in current_specialities %}
                    <option value="{{ spe.id }}" selected>{{ spe.name }}</option>
                 {% else %}
                    <option value="{{ spe.id }}">{{ spe.name }}</option>
                 {% endif %}
            {% endfor %}
        {% endif %}
    </select>
 </div>


<div class="form-group">
<label>Zone</label>
   <select id="zone" class="chosen-select" name="zone" disabled>
                    <!--{% if request.user.is_staff %} disabled{% endif %}>-->
                <option value="">--Select Zone--</option>
                {% if zone_obj %}
                    {% for r in zone_obj %}
                        {% if current_zone %}
                            {% if r.id == current_zone.id %}
                            <option value="{{ r.id }}" selected>{{ r.name }}</option>
                            {% else %}
                            <option value="{{ r.id }}">{{ r.name }}</option>
                            {% endif %}
                        {% else %}
                         <option value="{{ r.id }}">{{ r.name }}</option>
                        {% endif %}
                    {% endfor %}
                {% endif %}
   </select>
</div>


<div class="form-group">
<label>Zone Location</label>
    <select id="zone_location" class="chosen-select" name="zone_location" disabled>
                <!--{% if request.user.is_staff %} disabled{% endif %}>-->
            <option value="">--Select Zone Location--</option>
            {% if current_zone and zone_based_locations %}
                {% for i in zone_based_locations %}
                   {% if live_doctor.zone_location.id == i.id %}
                       <option value="{{ i.id }}" selected>{{ i.name }}</option>
                   {% else %}
                       <option value="{{ i.id }}">{{ i.name }}</option>
                   {% endif %}
                {% endfor %}

            {% elif current_zone %}
               <option value="{{ current_zone.id }}" selected>{{ current_zone.name }}</option>
            {% else %}
               <option value=""></option>
            {% endif %}




    </select>
</div>

<div class="form-group">
    <label>DID</label>
    <input type="text" maxlength="11" class="form-control" name="live_did" placeholder="DID" value="{{ live_doctor.did }}" readonly>
</div>

<div class="form-group">
    <label>Extension</label>
    <input type="text" maxlength="4" class="form-control" name="live_extension" placeholder="Extension" value="{{ live_doctor.extension }}" readonly>
</div>


</div>

</div>
<!--ONE END-->

<p>&nbsp;</p>
<h1 class="login_detils position_relative"><span class="font_weight_700 text_666">Contact Info</span></h1>
<p>&nbsp;</p>
<!--ONE START-->
<div class="row">
<div class="col-md-6">
<div class="form-group form-group-lg">
<input type="text" class="form-control" pattern = "^[1-9][0-9]*$" maxlength="10" minlength="10" placeholder = "Phone No" value = "{{live_doctor.phoneNo}}" name = "phone" readonly>
</div>
</div>

<div class="col-md-6">
<div class="form-group form-group-lg">
<input type="text" class="form-control" placeholder = "Fax" value = "{{live_doctor.fax}}" name = "fax" maxlength="15" minlength="10" pattern="[^a-zA-Z]*" title="Only Numbers and Special Characters allowed " readonly>
</div>
</div>

</div>
<!--ONE END-->

<!--ONE START-->
<div class="row">
<div class="col-md-6">
<div class="form-group form-group-lg">
<label>Mobile no<span style="color: red;size: 25px;">*</span> </label>
<input type="text" class="form-control" pattern = "^[1-9][0-9]*$" maxlength="10" minlength="10" placeholder = "Mobile No" value = "{{live_doctor.mobileNo}}" name = "mobile" readonly>
</div>
</div>

<div class="col-md-6">
<div class="form-group form-group-lg">
<input type="text" class="form-control" placeholder = "Skype ID" value = "{{ live_doctor.skypeId }}" name = "skype" pattern=".{8,50}"  title="8 to 50 characters" readonly>
</div>
</div>
</div>
<!--ONE END-->

<!--ONE START-->
<div class="row">
<div class="col-md-6">
<div class="form-group form-group-lg">
<label>Email ID<span style="color: red;size: 25px;">*</span> </label>
<input type="email" class="form-control" placeholder = "Email ID" value = "{{ live_doctor.email }}" name = "email" readonly>
</div>
</div>

<div class="col-md-6">
<div class="form-group form-group-lg">
<input type="email" class="form-control" placeholder = "Secondary Email ID" value = "{{ live_doctor.alternateEmail }}" name = "secondary_email" readonly>
</div>
</div>

<div class="col-md-6">
<div class="form-group form-group-lg">
<!--<button  style="margin-top: 15px;!important;" type="submit" class="btn btn-success ">Submit Basic Info</button>-->
</div>
</div>


</div>
<!--ONE END-->
</form>
