<div class="table-responsive table_modify_for_all p_tb_10">
      <table class="table table-bordered m_b_0">
         <thead class="bg_ededed">
            <tr>
               <th><input id="checkAll" type="checkbox"/></th>
               <th>Sr. No.</th>
               <th>Therapist Name</th>
               <th>City</th>
               <th>Location</th>
               <th>Stage</th>
               <th>Current User</th>
               <th>Free Text</th><th>CreatedAt</th>
                        <th>UpdatedAt</th>
               <th>Action</th>
            </tr>
         </thead>
         <tbody>
            {% for i in therapist_obj %}
            <tr>
               <td><input type="checkbox" value="{{ i.id }}"></td>
               <td>{{ forloop.counter }}</td>
               <td>{{ i.name | capfirst }}</td>
               <td>{{ i.city.name | capfirst }}</td>
                <td>{{ i.locality.name | capfirst }}</td>
                <td>{{ i.stage.stage_name | capfirst }}</td>

                <td>{{ i.current_user.username | capfirst }}</td>
                <td>{{ i.free_text | capfirst }}</td>
                <td>{{ i.createdAt }}</td>
                <td>{{ i.updatedAt }}</td>
                {% if i.stage_id != 5 %}<td>
                    <a class="text_white" href="{% url 'therapist-listing-edit' i.id %}?tab=1"><i
                        class="fa fa-edit"></i></a></td>
                {% else %}
                <td></td>
                {% endif %}
            </tr>
            {% endfor %}
         </tbody>
      </table>
      {% if therapist_obj %}
      <div class="text-right">
         <ul class="pagination">
            {% if therapist_obj.has_previous %}
            <li class="prev"><a href="?page={{ therapist_obj.previous_page_number }}{% if stage_data_pub %}&stage_data_pub={{stage_data_pub}}{% endif %}{% if search_name %}&search_name={{search_name}}{% endif %}">Prev</a> </li>
            {% endif %}
            <li><a href="">Page {{ therapist_obj.number }} of {{ therapist_obj.paginator.num_pages }}</a> </li>
            {% if therapist_obj.has_next %}
            <li class="next"><a href="?page={{ therapist_obj.next_page_number }}{% if stage_data_pub %}&stage_data_pub={{stage_data_pub}}{% endif %}{% if search_name %}&search_name={{search_name}}{% endif %}">Next</a> </li>
            {% endif %}
         </ul>
      </div>
      {% endif %}
   </div>